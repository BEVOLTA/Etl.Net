{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "Paillave.Etl.sln"
      ],
      "problemMatcher": "$msCompile",
      "options": {
        "cwd": "${workspaceFolder}/src"
      },
      "group": "build"
    },
    {
      "label": "build apps",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "FundProcess.Pms.Imports.sln"
      ],
      "problemMatcher": "$msCompile",
      "options": {
        "cwd": "${workspaceFolder}/src/FundProcess.Pms.Imports"
      },
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "clean",
      "command": "dotnet",
      "type": "process",
      "args": [
        "clean",
        "Paillave.Etl.sln"
      ],
      "problemMatcher": "$msCompile",
      "options": {
        "cwd": "${workspaceFolder}/src"
      }
    },
    {
      "label": "clean apps",
      "command": "dotnet",
      "type": "process",
      "args": [
        "clean",
        "FundProcess.Pms.Imports.sln"
      ],
      "problemMatcher": "$msCompile",
      "options": {
        "cwd": "${workspaceFolder}/src/FundProcess.Pms.Imports"
      }
    },
    {
      "label": "build documentation",
      "command": "docfx",
      "type": "process",
      "args": [
        "docfx_project/docfx.json",
        "--serve"
      ],
      "problemMatcher": []
    }
  ]
}