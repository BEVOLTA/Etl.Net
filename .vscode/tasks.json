{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/src/Paillave.Etl.sln"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "clean",
      "command": "dotnet",
      "type": "process",
      "args": [
        "clean",
        "${workspaceFolder}/src/Paillave.Etl.sln"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "build documentation",
      "command": "docfx",
      "type": "process",
      "args": [
        "docfx_project/docfx.json",
        "--serve"
      ],
      "problemMatcher": []
    }
  ]
}
