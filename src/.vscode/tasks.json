{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "Paillave.Etl.sln"
      ],
      "problemMatcher": "$msCompile",
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "clean",
      "command": "dotnet",
      "type": "process",
      "args": [
        "clean",
        "Paillave.Etl.sln"
      ],
      "problemMatcher": "$msCompile",
      "options": {
        "cwd": "${workspaceFolder}"
      }
    },
    {
      "label": "buildApp",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "FundProcess.Pms.Processes.sln"
      ],
      "problemMatcher": "$msCompile",
      "options": {
        "cwd": "${workspaceFolder}/FundProcess.Pms.Processes"
      },
      "group":"build"
    },
    {
      "label": "cleanApp",
      "command": "dotnet",
      "type": "process",
      "args": [
        "clean",
        "FundProcess.Pms.Processes.sln"
      ],
      "problemMatcher": "$msCompile",
      "options": {
        "cwd": "${workspaceFolder}/FundProcess.Pms.Processes"
      }
    },
  ]
}