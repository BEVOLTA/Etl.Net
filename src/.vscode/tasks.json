{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "Paillave.Etl.sln"
      ],
      "problemMatcher": "$msCompile",
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "clean",
      "command": "dotnet",
      "type": "process",
      "args": [
        "clean",
        "Paillave.Etl.sln"
      ],
      "problemMatcher": "$msCompile",
      "options": {
        "cwd": "${workspaceFolder}"
      }
    },
    {
      "label": "test",
      "command": "dotnet",
      "type": "shell",
      "args": [
        "test",
        "-p:CollectCoverage=true",
        "-p:CoverletOutputFormat=lcov",
        "-p:CoverletOutput=./lcov.info",
        "-p:Exclude=\"[xunit*]*\""
      ],
      "options": {
        "cwd": "${workspaceFolder}"
      }
    },
    {
      "label": "buildApp",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "FundProcess.Pms.Imports.sln"
      ],
      "problemMatcher": "$msCompile",
      "options": {
        "cwd": "${workspaceFolder}/FundProcess.Pms.Imports"
      },
      "group": "build"
    },
    {
      "label": "cleanApp",
      "command": "dotnet",
      "type": "process",
      "args": [
        "clean",
        "FundProcess.Pms.Imports.sln"
      ],
      "problemMatcher": "$msCompile",
      "options": {
        "cwd": "${workspaceFolder}/FundProcess.Pms.Imports"
      }
    },
  ]
}