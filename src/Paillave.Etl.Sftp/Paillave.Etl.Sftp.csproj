<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <PackageId>Etl.Net.Sftp</PackageId>
    <Version>1.0.0-alpha</Version>
    <Authors>St√©phane Royer</Authors>
    <Company></Company>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://paillave.github.io/Etl.Net/</PackageProjectUrl>
    <PackageTags>ETL .net core SSIS reactive SFTP</PackageTags>
    <Product>ETL.net SFTP extensions</Product>
    <PackageIcon>NugetIcon.png</PackageIcon>
    <Description>SFTP extensions for Etl.Net</Description>
    <PackageReleaseNotes>
extensions for Etl.Net to query against SFTP
    </PackageReleaseNotes>
    <LangVersion>latest</LangVersion>
  </PropertyGroup>
  <ItemGroup>
    <None Include="../NugetIcon.png" Pack="true" Visible="false" PackagePath="" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.FileSystemGlobbing" Version="3.1.8" />
    <PackageReference Include="SSH.NET" Version="2016.1.0" />
  </ItemGroup>

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Paillave.Etl\Paillave.Etl.csproj" />
  </ItemGroup>

</Project>
